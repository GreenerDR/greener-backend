{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "name": "location"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "phone": {
      "type": "biginteger"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "locationType": {
      "via": "locations",
      "model": "type"
    },
    "address": {
      "type": "string",
      "required": false
    },
    "url": {
      "type": "string"
    },
    "images": {
      "via": "location",
      "collection": "image"
    }
  }
}
